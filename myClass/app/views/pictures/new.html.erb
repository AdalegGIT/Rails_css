<h1>Add a picture</h1>



<div class="container">

<div class="card">
  <div class="card-body">
 
 <%= form_for @picture do |f| %>
  <div class="form-group">
    <%= f.label :artist %><br>
    <%= f.text_field :artist, class:"form-control" %>
  </div>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit "Save" , class: 'btn btn-primary' %>
  </p>
<% end %>

  </div>
</div>

</div>